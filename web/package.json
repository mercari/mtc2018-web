{
  "name": "mtc2018-web",
  "version": "0.0.1",
  "description": "Webpage for mercari Tech Conf 2018",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV='production' node server.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "codegen": "rm -rf graphql/generated && apollo codegen:generate graphql/generated --schema ../schema.graphql --addTypename --target typescript --outputFlat"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "author": "sawa-zen",
  "dependencies": {
    "@types/fontfaceobserver": "0.0.6",
    "@zeit/next-typescript": "^1.1.0",
    "apollo-boost": "^0.1.15",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "fontfaceobserver": "^2.0.13",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "graphql-tag": "^2.9.2",
    "i18next": "^17.0.7",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-express-middleware": "^1.8.0",
    "i18next-node-fs-backend": "^2.1.3",
    "i18next-xhr-backend": "^3.0.0",
    "intersection-observer": "^0.5.0",
    "isomorphic-unfetch": "^3.0.0",
    "moment": "^2.22.2",
    "next": "^7.0.2",
    "next-i18next": "^0.52.1",
    "next-routes": "^1.4.2",
    "react": "^16.8.6",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "styled-components": "^3.4.5",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/next": "^7.0.3",
    "@types/react": "^16.8.23",
    "@types/webpack": "^4.4.11",
    "@types/webpack-env": "^1.13.6",
    "apollo": "^1.9.2",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-plugin-styled-components": "^1.6.3",
    "copy-webpack-plugin": "^4.5.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0"
  }
}
